{
  "name": "sushiii",
  "version": "1.0.0",
  "description": "react sushii",
  "main": "lib/server.js",
  "repository": "jortiz31/sushii",
  "author": "joortiz",
  "license": "MIT",
  "scripts": {
    "dev": "pm2 start lib/server.js --watch --interpreter babel-node --name appDev",
    "webpack": "webpack -wd",
    "logs": "pm2 logs",
    "stop": "pm2 kill",
    "test": "jest --watchAll",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start": "pm2 build/server.js -i max --name appProd"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.10",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "pm2": "^2.10.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0"
  }
}
